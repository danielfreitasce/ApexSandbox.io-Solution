public Id insertCourse(String name, String details, Integer credits) {
    
    apxio__Credits__c apx_credits = new apxio__Credits__c();

    if(credits >=1 && credits <=4){
        apx_credits.Name = name;
        apx_credits.apxio__Course_Details__c = details;
        apx_credits.apxio__Credits__c = credits;
    }
    else{
        return null; 
    }

    insert apx_credits; 
    return apx_credits.Id; 
}