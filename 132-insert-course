public Id insertCourse(String name, String details, Integer credits) {
    
    apxio__Credits__c credits = new apxio__Credits__c();

    if(credits >=1 && credits <=4){
        credits.Name = name;
        credits.apxio__Course_Details__c = details;
        credits.apxio__Credits__c = credits;
    }
    else{
        return null; 
    }

    insert credits; 
    return credits.Id; 
}