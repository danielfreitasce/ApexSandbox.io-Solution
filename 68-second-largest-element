public Integer secondLargest(List<Integer> numbers) {
    
     
    Integer num = -2147483647 - 1;
    Integer f = num; 
    Integer s = num; 

    for(Integer i = 0; i < numbers.size(); i++){
        if(numbers[i] > f){
            s = f; 
            f = numbers[i];
        }
        else if(numbers[i] > s){
            s = numbers[i];
        }
    }
    if(s > f){
        Integer aux = s; 
        s = f;
        f = aux; 
    }
    return s;
}