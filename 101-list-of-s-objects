public List<sObject> getListOfsObject(List<Account> accounts, List<Contact> contacts)
{
    List<sObject> sObjectList = new List<sObject>();   
    
    if(accounts.isEmpty() && contacts.isEmpty()){
        return sObjectList;
    } 
    
    if(accounts != null){
        sObjectList.addAll(accounts);
    }
    if(contacts != null){
        sObjectList.addAll(contacts);
    }

    return sObjectList; 
}